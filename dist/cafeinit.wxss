view {
  box-sizing: border-box;
}
/**
 * @fileoverview common
 * @author: burning <www.cafeinit.com>
 * @version: 2017-02-22
 */
.ci-fixed-top {
  z-index: 500;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  margin: 0!important;
}
.ci-fixed-bottom {
  z-index: 500;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  margin: 0!important;
}
/**
 * @fileoverview ci-block
 * @author: burning <www.cafeinit.com>
 * @version: 2016-08-20
 */
.ci-block {
  position: relative;
  background-color: #fff;
  padding: 0;
  margin: 0 0 15px 0;
  border-top: 1px solid #ddd;
  border-bottom: 1px solid #ddd;
}
.ci-block:before,
.ci-block:after {
  content: " ";
  display: table;
}
.ci-block:after {
  clear: both;
}
.ci-block-header,
.ci-block-body,
.ci-block-footer {
  position: relative;
  padding: 10px 15px;
}
.ci-block-header:before,
.ci-block-body:before,
.ci-block-footer:before,
.ci-block-header:after,
.ci-block-body:after,
.ci-block-footer:after {
  content: " ";
  display: table;
}
.ci-block-header:after,
.ci-block-body:after,
.ci-block-footer:after {
  clear: both;
}
.ci-block-header {
  border-bottom: 1px solid #ddd;
  font-size: 14px;
}
.ci-block-footer {
  font-size: 14px;
}
.ci-block .ci-list {
  margin: 0;
  border-top: none;
  border-bottom: none;
}
.ci-block .ci-list-cell {
  padding-left: 0;
  padding-right: 0;
}
.ci-block .ci-list-cell::before {
  left: 0;
}
/**
 * @fileoverview ci-collection
 * @author: burning <www.cafeinit.com>
 * @version: 2017-01-11
 */
.ci-collection {
  padding: 0;
  margin: 0 0 15px 0;
}
.ci-collection-flex {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
}
.ci-collection-float::after {
  content: ' ';
  display: block;
  clear: both;
  width: 100%;
  height: 0;
  line-height: 0;
}
.ci-collection-cell {
  padding: 0;
  margin: 0;
  min-width: 50px;
  min-height: 50px;
}
.ci-collection-flex .ci-collection-cell {
  flex: 1;
  flex-basis: 50%;
}
@media only screen and (min-width:641px) {
  .ci-collection-flex .ci-collection-cell {
    flex-basis: 33.33333333%;
  }
}
@media only screen and (min-width:1025px) {
  .ci-collection-flex .ci-collection-cell {
    flex-basis: 25%;
  }
}
.ci-collection-float .ci-collection-cell {
  float: left;
  width: 50%;
}
@media only screen and (min-width:641px) {
  .ci-collection-float .ci-collection-cell {
    width: 33.33333333%;
  }
}
@media only screen and (min-width:1025px) {
  .ci-collection-float .ci-collection-cell {
    width: 25%;
  }
}
/**
 * @fileoverview ci-dimmer
 * @author: burning <www.cafeinit.com>
 * @version: 2016-11-28
 */
.ci-dimmer {
  z-index: 1000;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
}
/**
 * @fileoverview ci-modal
 * @author: burning <www.cafeinit.com>
 * @version: 2016-11-28
 */
.ci-modal {
  z-index: 1200;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  text-align: center;
  transition: opacity 0.3s ease;
}
.ci-modal:before {
  content: " ";
  display: inline-block;
  height: 100%;
  vertical-align: middle;
}
.ci-modal .ci-dimmer {
  position: absolute;
}
.ci-modal-dialog {
  z-index: 1500;
  position: relative;
  display: inline-block;
  vertical-align: middle;
  width: 260px;
  max-width: 100%;
  min-height: 44px;
  max-height: 100%;
  margin: auto;
  transition: all 0.3s ease;
}
.ci-modal-alert .ci-modal-dialog,
.ci-modal-confirm .ci-modal-dialog {
  background-color: #fff;
  border-radius: 10px;
}
.ci-modal-header {
  padding: 15px 15px 10px 15px;
  font-size: 12px;
  font-weight: 500;
}
.ci-modal-body {
  padding: 15px;
}
.ci-modal-footer {
  display: table;
  width: 100%;
  border-top: 1px solid #ddd;
  height: 44px;
  line-height: 44px;
}
.ci-modal-btn {
  display: table-cell;
  height: 44px;
  padding: 0 10px;
  border-right: 1px solid #ddd;
  text-decoration: none;
  overflow: hidden;
  word-wrap: normal;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.ci-modal-btn:last-child {
  border-right: none;
}
.modal-actions,
.ci-modal-actions {
  z-index: 1200;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  padding: 10px;
  transform: translateY(0);
  transition: all 0.3s ease;
}
.modal-actions-group,
.ci-modal-actions-group {
  background-color: #fff;
  overflow: hidden;
  min-height: 44px;
  margin-bottom: 10px;
  border-radius: 10px;
}
.modal-actions-group .ci-list,
.ci-modal-actions-group .ci-list {
  margin: 0;
}
.modal-actions-group .ci-list-cell:before,
.ci-modal-actions-group .ci-list-cell:before {
  left: 0;
}
.modal-actions-group a,
.ci-modal-actions-group a {
  display: block;
  text-decoration: none;
}
.modal-actions-group:last-child,
.ci-modal-actions-group:last-child {
  margin-bottom: 0;
}
.ci-enter,
.ci-leave-active {
  opacity: 0;
}
.ci-enter .ci-modal-container,
.ci-leave-active .ci-modal-container,
.ci-enter .ci-modal-dialog,
.ci-leave-active .ci-modal-dialog {
  transform: scale(0.75);
}
.ci-enter .modal-actions,
.ci-leave-active .modal-actions,
.ci-enter .ci-modal-actions,
.ci-leave-active .ci-modal-actions {
  transform: translateY(100%);
}
/* for bootstrap */
.modal {
  z-index: 1200;
  text-align: center;
}
.modal:before {
  content: " ";
  display: inline-block;
  height: 100%;
  vertical-align: middle;
}
.modal-dialog {
  z-index: 1500;
  position: relative;
  display: inline-block;
  vertical-align: middle;
  width: 80%;
  min-width: 260px;
  max-width: 100%;
  min-height: 44px;
  max-height: 100%;
  margin: auto;
  text-align: left;
}
/**
 * @fileoverview ci-list
 * @author: burning <www.cafeinit.com>
 * @version: 2016-08-19
 */
.ci-list-header,
.ci-list-footer {
  padding: 5px 10px;
  font-size: 14px;
  color: #555555;
}
.ci-list {
  list-style: none;
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  padding: 0;
  margin: 0 0 15px 0;
  border-top: 1px solid #ddd;
  border-bottom: 1px solid #ddd;
}
.ci-list-cell {
  list-style: none;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  min-height: 44px;
  padding: 10px 15px;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  background-color: #fff;
}
.ci-list-cell:before {
  content: " ";
  position: absolute;
  left: 15px;
  top: 0;
  right: 0;
  height: 0;
  border-top: 1px solid #ddd;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.ci-list-cell:first-child:before {
  border-top: none;
}
.ci-list-cell-body {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.ci-list-cell-body .title,
.ci-list-cell-body .sub-title {
  display: block;
  float: left;
  width: 45%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-weight: normal;
}
.ci-list-cell-body .sub-title {
  float: right;
  width: 50%;
  text-align: right;
  color: #999999;
}
.ci-list-cell-accessory {
  margin-left: 0.5em;
  text-align: center;
  color: #999999;
}
/**
 * @fileoverview ci-tabs
 * @author: burning <www.cafeinit.com>
 * @version: 2017-02-16
 */
.ci-tabs {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  padding: 0;
  margin: 0 0 15px 0;
  border-top: 1px solid #ddd;
  border-bottom: 1px solid #ddd;
  background-color: #fff;
}
.ci-tab {
  flex: 1;
  position: relative;
  padding: 10px;
  text-align: center;
}
.ci-tab-title {
  display: inline-block;
  font-size: 14px;
  line-height: 1.25em;
}
.ci-tab-active .ci-tab-title {
  color: #1abc9c;
}
.ci-tabs-underline .ci-tab-active {
  border-bottom: 2px solid #1abc9c;
}
.ci-tabs-column .ci-tab {
  border-left: 1px solid #eaeaea;
}
.ci-tabs-column .ci-tab:first-child {
  border-left-width: 0;
}
.ci-tabs-bar .ci-icon {
  display: block;
  width: 24px;
  height: 24px;
  margin: 0 auto;
  font-size: 24px;
}
.ci-tabs-bar .ci-tab-title {
  display: block;
  font-size: 12px;
  line-height: 1.25em;
}
/**
 * @fileoverview ci-toast
 * @author: burning <www.cafeinit.com>
 * @version: 2016-11-22
 */
.ci-toast {
  z-index: 1500;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  outline: 0;
  text-align: center;
  transition: all 0.3s ease;
}
.ci-toast::before {
  content: ' ';
  display: inline-block;
  height: 100%;
  vertical-align: middle;
}
.ci-toast-content {
  z-index: 1200;
  position: relative;
  display: inline-block;
  min-width: 120px;
  max-width: 80%;
  padding: 15px;
  margin-top: -20%;
  border-radius: 10px;
  vertical-align: middle;
  text-align: center;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.75);
  box-shadow: 1px 1px 10px 3px rgba(0, 0, 0, 0.25);
  transform: translateY(0);
  transition: all 0.3s ease;
}
.ci-toast-icon {
  display: block;
  width: 48px;
  height: 48px;
  margin: 0 auto 10px auto;
  font-weight: lighter;
  font-size: 43.2px;
  line-height: 48px;
}
.ci-enter,
.ci-leave-active {
  opacity: 0;
}
.ci-enter .ci-toast,
.ci-leave-active .ci-toast {
  transform: scale(0.75);
}
.ci-enter .ci-toast-content,
.ci-leave-active .ci-toast-content {
  transform: translateY(-50%);
}
